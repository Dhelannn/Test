game.StarterGui:SetCore("SendNotification", {
Title = "[TerminalX]",
Text = "Press F9 or type /console in chat.",
Duration = 15
})

task.spawn(function()
warn("[TerminalX] This is Loadstring is only for Tests. The Main One is now copied to your clipboard")

local loadstringText = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/Dhelannn/Test/refs/heads/main/TerminalX"))()'

if setclipboard then
setclipboard(loadstringText)
else
warn("[TerminalX] setclipboard not supported on this executor.")
end
end)

game.StarterGui:SetCore("SendNotification", {
Title = "[TerminalX]",
Text = "Press F9 or type /console in chat.",
Duration = 15
})

task.spawn(function()
warn("[TerminalX] This Loadstring is a Test Loadstring and doesn't do anything. The Main and Functional One is now copied to your clipboard")

local loadstringText = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/Dhelannn/TerminalX/refs/heads/main/1.0"))()'

if setclipboard then
setclipboard(loadstringText)
else
warn("[TerminalX] setclipboard not supported on this executor.")
end
end)
